    "devDependencies": {
        "babelify": "^6.3.0",
        "browser-sync": "^2.9.11",
        "browserify": "^11.2.0",
        "del": "^2.0.2",
        "gulp": "^3.9.0",
        "gulp-autoprefixer": "^3.1.0",
        "gulp-bump": "^1.0.0",
        "gulp-cachebust": "0.0.6",
        "gulp-gzip": "^1.2.0",
        "gulp-imagemin": "^2.3.0",
        "gulp-rev": "^6.0.1",
        "gulp-rev-replace": "^0.4.2",
        "gulp-sass": "^2.0.4",
        "gulp-sourcemaps": "^1.6.0",
        "gulp-svn": "^1.0.6",
        "gulp-tar": "^1.5.0",
        "gulp-uglify": "^1.4.2",
        "gulp-util": "^3.0.6",
        "imagemin-pngquant": "^4.2.0",
        "lodash.assign": "^3.2.0",
        "vinyl-buffer": "^1.0.0",
        "vinyl-paths": "^2.0.0",
        "vinyl-source-stream": "^1.1.0",
        "watchify": "^3.4.0"
    },
    "dependencies": {
        "lodash": "^3.10.0",
        "jquery": "^2.1.4",
        "slick-carousel": "^1.5.5"
    },
    "browserify": {
        "transform": [
            "babelify"
        ]
